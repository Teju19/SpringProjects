package com.hcl.springdemo;

public class employee {
	
	private String eName;
	private int eId;
	
	public String geteName() {
		return eName;
	}
	public void seteName(String eName) {
		this.eName = eName;
	}
	public int geteId() {
		return eId;
	}
	public void seteId(int eId) {
		this.eId = eId;
	}
	
	public employee() {
		super();
		// TODO Auto-generated constructor stub
	}
	
	
	
	
}
